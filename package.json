{
  "name": "Boilerplate",
  "version": "0.0.1",
  "private": true,
  "main": "server/index.js",
  "scripts": {
    "build": "NODE_ENV=production rm -rf dist && BABEL_ENV=production babel-node ./node_modules/.bin/webpack --stats --config ./webpack/prod.config.js",
    "dev": "NODE_ENV=development babel-node ./webpack/dev-server.js",
    "test": "NODE_ENV=test NODE_PATH=app nyc --cache --extension .jsx ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint app server webpack shared test --ext .jsx,.js"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "./test/helpers/setup-browser-env",
      "babel-register",
      "babel-polyfill"
    ]
  },
  "nyc": {
    "exclude": [
      "**/*.test.js",
      "server/**/*",
      "app/data/*"
    ]
  },
  "dependencies": {
